<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','RecieveName','$event',[]],['addressData']]]]]}}" value="{{addressData.RecieveName}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','MobilePhone','$event',[]],['addressData']]]]]}}" value="{{addressData.MobilePhone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><text class="input">{{''+addressData.Address+''}}</text><text data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="yticon icon-shouhuodizhi" bindtap="__e"></text></view><view class="row b-b"><text class="tit">门牌号</text><input class="input" type="text" placeholder="楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','AddressDetail','$event',[]],['addressData']]]]]}}" value="{{addressData.AddressDetail}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.IsDefault==1}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>